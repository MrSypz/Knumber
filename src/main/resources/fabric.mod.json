{
  "schemaVersion": 1,
  "id": "knumber",
  "version": "${version}",
  "name": "Knumber",
  "description": "A standalone module of Penomior focused on text, damage, and number particles. It displays animated floating text above entities, featuring smooth damage numbers, critical hit effects, miss notifications, and back attack indicators.",

  "authors": [
    "Sypztep"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/knumber/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "sypztep.knumber.client.KnumberClient"
    ],
    "modmenu": [
      "sypztep.knumber.client.compat.ModMenuIntegration"
    ]
  },
  "mixins": [
    "knumber.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config2": ">=${cloth_config_version}"
  }
}
